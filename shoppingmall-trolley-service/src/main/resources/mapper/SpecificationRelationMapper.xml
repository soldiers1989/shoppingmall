<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhkj.mapper.specificationsrelation_mapper.SpecificationsRelationMapper">
    <resultMap id="resultMap" type="com.zhkj.entity.SpecificationsrelationEntity">
        <id property="id" column="id"/>
        <result property="commodityNumber" column="commodityNumber"/>
        <result property="commodityPrice" column="commodityPrice"/>
        <result property="commodityId" column="commodityId"/>
        <result property="smallPictureUrl" column="smallPictureUrl"/>
        <result property="typeId" column="typeId"/>
        <result property="speciTopicId" column="speciTopicId"/>
        <result property="speciDetailedId" column="speciDetailedId"/>
    </resultMap>
    <select id="selectMaxByCommodityId" resultType="map" resultMap="resultMap">
        SELECT * FROM specificationsrelation WHERE commodityId=#{commodityId}
    </select>
</mapper>