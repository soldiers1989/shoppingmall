<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhkj.mapper.type_mapper.TypeMapper">
       <resultMap id="resultMap" type="com.zhkj.entity.TypeEntity">
           <id property="id" column="id"/>
           <result property="typeName" column="typeName"/>
       </resultMap>
    <select id="selectAllTypeName" resultMap="resultMap">
        SELECT * FROM type limit 10
    </select>
</mapper>