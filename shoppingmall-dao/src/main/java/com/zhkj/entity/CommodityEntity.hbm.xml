<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.entity.CommodityEntity" table="commodity" schema="shoppingmall">
        <property name="id" column="id"/>
        <property name="commodityName" column="commodityName"/>
        <property name="commodityIntroduce" column="commodityIntroduce"/>
        <property name="bigPictureUrl" column="bigPictureUrl"/>
        <property name="commodityTypeRelationId" column="commodityTypeRelationId"/>
    </class>
</hibernate-mapping>