<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.entity.AuthenticationEntity" table="authentication" schema="shoppingmall">
        <property name="id" column="id"/>
        <property name="userName" column="userName"/>
        <property name="userAboutAddress" column="userAboutAddress"/>
        <property name="userAddress" column="userAddress"/>
        <property name="userPhoneNumber" column="userPhoneNumber"/>
    </class>
</hibernate-mapping>