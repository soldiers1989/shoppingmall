<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.entity.ShoppingcartEntity" table="shoppingcart" schema="shoppingmall">
        <property name="id" column="id"/>
        <property name="commdityNumber" column="commdityNumber"/>
        <property name="commdityPrice" column="commdityPrice"/>
        <property name="commdityId" column="commdityId"/>
        <property name="userId" column="userId"/>
    </class>
</hibernate-mapping>