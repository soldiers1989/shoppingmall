<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhkj.entity.ShoppingcartEntity" table="shoppingcart" schema="shoppingmall">
        <property name="id" column="id"/>
        <property name="commodityNumber" column="commodityNumber"/>
        <property name="commodityPrice" column="commodityPrice"/>
        <property name="commodityId" column="commodityId"/>
        <property name="smallPictureUrl" column="smallPictureUrl"/>
        <property name="commodityIntroduce" column="commodityIntroduce"/>
        <property name="userId" column="userId"/>
    </class>
</hibernate-mapping>