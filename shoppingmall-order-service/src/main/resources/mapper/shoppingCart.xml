<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhkj.mapper.order_mapper.ShoppingCartMappers">
    <select id="selectByUserId" resultType="com.zhkj.entity.ShoppingcartEntity">
        SELECT * FROM shoppingcart
        WHERE userId = #{userId}
    </select>
</mapper>